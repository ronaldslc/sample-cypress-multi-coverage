{
  "private": true,
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_CONFIG=./cypress/configs/$TARGET_APP.config.js && cypress run --config-file $CYPRESS_CONFIG",
    "cy:run:coverage": "nyc yarn run cy:run",
    "pretest": "rimraf .nyc_output .cache coverage dist",
    "start": "CYPRESS_CONFIG=./cypress/configs/$TARGET_APP.config.js START_COMMAND=`node -p \"require('$CYPRESS_CONFIG').startCommand\"` && cd ./$TARGET_APP && BROWSER=none $START_COMMAND",
    "test": "CYPRESS_CONFIG=./cypress/configs/$TARGET_APP.config.js BASE_URL=`node -p \"require('$CYPRESS_CONFIG').e2e.baseUrl\"` && start-server-and-test start $BASE_URL cy:run",
    "test:coverage": "CYPRESS_CONFIG=./cypress/configs/$TARGET_APP.config.js BASE_URL=`node -p \"require('$CYPRESS_CONFIG').e2e.baseUrl\"` && start-server-and-test start $BASE_URL cy:run:coverage",
    "test:watch": "CYPRESS_CONFIG=./cypress/configs/$TARGET_APP.config.js yarn run cypress open --config-file $CYPRESS_CONFIG",
    "instrument": "cp -r ./$TARGET_APP/src instrumented && nyc instrument --compact false ./$TARGET_APP/src instrumented"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.3",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.9.0",
    "nyc": "^15.1.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {},
  "license": "SEE LICENSE IN LICENSE.md",
  "workspaces": ["reactfnd", "angularjsfnd"]
}
